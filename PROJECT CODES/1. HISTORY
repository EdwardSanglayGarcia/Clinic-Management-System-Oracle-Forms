SELECT * FROM GUARDIAN

DELETE FROM INFORMATION

SELECT * FROM INFORMATION
SELECT * FROM TYPE


SELECT 
CLINIC.FIRST_NAME, CLINIC.LAST_NAME, 
INFORMATION.GUARDIAN_ID, INFORMATION.NATIONALITY,
GUARDIAN.FIRST_NAME, GUARDIAN.LAST_NAME
FROM CLINIC
INNER JOIN INFORMATION ON CLINIC.PERSON_ID = INFORMATION.PERSON_ID
AND CLINIC.TYPE_ID = 1
INNER JOIN GUARDIAN ON INFORMATION.GUARDIAN_ID = GUARDIAN.GUARDIAN_ID


DROP TABLE GUARDIAN

SELECT CLINIC.FIRST_NAME||' '||CLINIC.LAST_NAME AS FULL_NAME, INFORMATION.NATIONALITY,
PHYSICALEXAM.HEART_RATE,
PHYSICALEXAMHISTORY.PE_DATE,
PERSONINCHARGE.FIRST_NAME ||' '||PERSONINCHARGE.LAST_NAME AS PERSON_IN_CHARGE
FROM CLINIC
INNER JOIN INFORMATION ON CLINIC.PERSON_ID = INFORMATION.PERSON_ID
INNER JOIN PHYSICALEXAM ON INFORMATION.PERSON_ID = PHYSICALEXAM.PERSON_ID
INNER JOIN PHYSICALEXAMHISTORY ON PHYSICALEXAM.PE_NO = PHYSICALEXAMHISTORY.PE_NO
INNER JOIN PERSONINCHARGE ON PHYSICALEXAMHISTORY.PIC_ID = PERSONINCHARGE.PIC_ID

//USE AS A DECLARED VALUE FOR THE EXPORTATION OF DATA
DECLARE
VAR_FN VARCHAR(99);
VAR_LN VARCHAR(99);
VAR_G VARCHAR(99);
VAR_C VARCHAR(99);
VAR_GN VARCHAR(99);
VAR_YR INT;
VAR_CC VARCHAR(99);
VAR_CN VARCHAR(99);
CURSOR CUR_TRIAL IS 
SELECT CLINIC.FIRST_NAME, CLINIC.LAST_NAME,
INFORMATION.GENDER, INFORMATION.CAMPUS,
GUARDIAN.FIRST_NAME||' '||GUARDIAN.LAST_NAME AS GUARDIAN_NAME,
STUDENT.STUDENT_YEAR, COURSE.COURSE_CODE, COURSE.COURSE_NAME
FROM CLINIC
INNER JOIN INFORMATION ON CLINIC.PERSON_ID = INFORMATION.PERSON_ID
INNER JOIN GUARDIAN ON INFORMATION.GUARDIAN_ID = GUARDIAN.GUARDIAN_ID
INNER JOIN STUDENT ON CLINIC.PERSON_ID = STUDENT.PERSON_ID
INNER JOIN COURSE ON STUDENT.COURSE_ID = COURSE.COURSE_ID;

BEGIN
OPEN CUR_TRIAL;
LOOP	
FETCH CUR_TRIAL INTO VAR_FN, VAR_LN, VAR_G, VAR_C, VAR_GN, VAR_YR, VAR_CC, VAR_CN;

EXIT WHEN CUR_TRIAL%NOTFOUND;
DBMS_OUTPUT.PUT_LINE(VAR_FN||' '||VAR_LN);
DBMS_OUTPUT.PUT_LINE(VAR_G||' '||VAR_C);
DBMS_OUTPUT.PUT_LINE(VAR_GN);
DBMS_OUTPUT.PUT_LINE(VAR_YR);
DBMS_OUTPUT.PUT_LINE(VAR_CC);
DBMS_OUTPUT.PUT_LINE(VAR_CN);
DBMS_OUTPUT.PUT_LINE('');
END LOOP;
CLOSE CUR_TRIAL;
END;

SELECT 
CLINIC.LAST_NAME, CLINIC.FIRST_NAME, CLINIC.PERSON_ID,
INFORMATION.NATIONALITY, INFORMATION.BIRTH_DATE,
GUARDIAN.LAST_NAME||', '|| GUARDIAN.FIRST_NAME AS GUARDIAN_NAME
FROM CLINIC
INNER JOIN INFORMATION ON CLINIC.PERSON_ID = INFORMATION.PERSON_ID
INNER JOIN GUARDIAN ON INFORMATION.GUARDIAN_ID = GUARDIAN.GUARDIAN_ID